<?xml version="1.0" encoding="utf-8"?>
<com.jsmosce.view.percentsuppor.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    android:orientation="vertical"
    tools:context="com.jsmosce.activity.MeActivity">


    <com.jsmosce.view.TitleView
        android:id="@+id/me_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titleText="我的" />


    <com.jsmosce.view.percentsuppor.PercentRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@drawable/shape"
        android:gravity="bottom"
        android:orientation="vertical"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:theme="@style/ThemeOverlay.AppCompat.Dark">

        <TextView
            android:id="@+id/home_userName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/home_userImg"
            android:text="用户名"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textColor="@color/colorBlack"
            app:layout_textSizePercent="@string/app_dp_18" />

        <TextView
            android:id="@+id/home_yiyuan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_textSizePercent="@string/app_dp_16"
            android:layout_below="@+id/home_userName"
            android:layout_toRightOf="@+id/home_userImg"
            android:text="所属科室"
            android:textColor="@color/colorBlack" />

    </com.jsmosce.view.percentsuppor.PercentRelativeLayout>

    <com.jsmosce.view.percentsuppor.PercentRelativeLayout
        android:id="@+id/nav_gallery"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_heightPercent="@string/app_dp_50">

        <View
            android:id="@+id/me_item_top1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_lishi"
            app:layout_heightPercent="@string/app_dp_25"
            app:layout_marginLeftPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_25" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/me_item_top1"
            android:text="历史记录"
            app:layout_textSizePercent="@string/app_dp_16"
            android:textColor="@color/colorBlack"
            app:layout_marginLeftPercent="@string/app_dp_24" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_rightarrow"
            app:layout_heightPercent="@string/app_dp_20"
            app:layout_marginRightPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_20" />

        <View
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@color/colorGrayLight"
            app:layout_heightPercent="@string/app_dp_1"
            app:layout_marginLeftPercent="@string/app_dp_14" />
    </com.jsmosce.view.percentsuppor.PercentRelativeLayout>

    <com.jsmosce.view.percentsuppor.PercentRelativeLayout
        android:id="@+id/nav_manage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_heightPercent="@string/app_dp_50">

        <View
            android:id="@+id/me_item_top2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_zhanghao"
            app:layout_textSizePercent="@string/app_dp_16"
            app:layout_heightPercent="@string/app_dp_25"
            app:layout_marginLeftPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_25" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/me_item_top2"
            android:text="修改密码"
            app:layout_textSizePercent="@string/app_dp_16"
            android:textColor="@color/colorBlack"
            app:layout_marginLeftPercent="@string/app_dp_24" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_rightarrow"
            app:layout_heightPercent="@string/app_dp_20"
            app:layout_marginRightPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_20" />

        <View
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@color/colorGrayLight"
            app:layout_heightPercent="@string/app_dp_1"
            app:layout_marginLeftPercent="@string/app_dp_14" />
    </com.jsmosce.view.percentsuppor.PercentRelativeLayout>


    <com.jsmosce.view.percentsuppor.PercentRelativeLayout
        android:id="@+id/icon_updata"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_heightPercent="@string/app_dp_50">

        <View
            android:id="@+id/me_item_top3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_updata"
            app:layout_heightPercent="@string/app_dp_25"
            app:layout_marginLeftPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_25" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/me_item_top3"
            android:text="检查更新"
            app:layout_textSizePercent="@string/app_dp_16"
            android:textColor="@color/colorBlack"
            app:layout_marginLeftPercent="@string/app_dp_24" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_rightarrow"
            app:layout_heightPercent="@string/app_dp_20"
            app:layout_marginRightPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_20" />

        <View
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@color/colorGrayLight"
            app:layout_heightPercent="@string/app_dp_1"
            app:layout_marginLeftPercent="@string/app_dp_14" />
    </com.jsmosce.view.percentsuppor.PercentRelativeLayout>


    <com.jsmosce.view.percentsuppor.PercentRelativeLayout
        android:id="@+id/nav_feedback"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_heightPercent="@string/app_dp_50">

        <View
            android:id="@+id/me_item_top4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_yijianfankui"
            app:layout_heightPercent="@string/app_dp_25"
            app:layout_marginLeftPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_25" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/me_item_top4"
            android:text="意见反馈"
            app:layout_textSizePercent="@string/app_dp_16"
            android:textColor="@color/colorBlack"
            app:layout_marginLeftPercent="@string/app_dp_24" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_rightarrow"
            app:layout_heightPercent="@string/app_dp_20"
            app:layout_marginRightPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_20" />

        <View
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@color/colorGrayLight"
            app:layout_heightPercent="@string/app_dp_1"
            app:layout_marginLeftPercent="@string/app_dp_14" />
    </com.jsmosce.view.percentsuppor.PercentRelativeLayout>


    <com.jsmosce.view.percentsuppor.PercentRelativeLayout
        android:id="@+id/nav_camera"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_heightPercent="@string/app_dp_50">

        <View
            android:id="@+id/me_item_top5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_clear"
            app:layout_heightPercent="@string/app_dp_25"
            app:layout_marginLeftPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_25" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/me_item_top5"
            android:text="清除缓存"
            android:textColor="@color/colorBlack"
            app:layout_textSizePercent="@string/app_dp_16"
            app:layout_marginLeftPercent="@string/app_dp_24" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_rightarrow"
            app:layout_heightPercent="@string/app_dp_20"
            app:layout_marginRightPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_20" />

        <View
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@color/colorGrayLight"
            app:layout_heightPercent="@string/app_dp_1"
            app:layout_marginLeftPercent="@string/app_dp_14" />
    </com.jsmosce.view.percentsuppor.PercentRelativeLayout>


    <com.jsmosce.view.percentsuppor.PercentRelativeLayout
        android:id="@+id/nav_exit"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_heightPercent="@string/app_dp_50">

        <View
            android:id="@+id/me_item_top6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_exit"
            app:layout_heightPercent="@string/app_dp_25"
            app:layout_marginLeftPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_25" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            app:layout_textSizePercent="@string/app_dp_16"
            android:layout_toRightOf="@+id/me_item_top6"
            android:text="退出登录"
            android:textColor="@color/colorBlack"
            app:layout_marginLeftPercent="@string/app_dp_24" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/icon_rightarrow"
            app:layout_heightPercent="@string/app_dp_20"
            app:layout_marginRightPercent="@string/app_dp_14"
            app:layout_widthPercent="@string/app_dp_20" />

        <View
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@color/colorGrayLight"
            app:layout_heightPercent="@string/app_dp_1"
            app:layout_marginLeftPercent="@string/app_dp_14" />
    </com.jsmosce.view.percentsuppor.PercentRelativeLayout>


</com.jsmosce.view.percentsuppor.PercentLinearLayout>
